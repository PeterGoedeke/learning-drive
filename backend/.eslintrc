{
    "parser": "@typescript-eslint/parser",
    "extends": ["prettier", "plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "@typescript-eslint/ban-types": "off",
        "@typescript-eslint/no-explicit-any": "off"
    }
}
